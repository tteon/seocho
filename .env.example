# SEOCHO Environment Variables
# Copy this file to .env and fill in your values:
#   cp .env.example .env

# GitHub
GITHUB_TOKEN=ghp_your_token_here

# OpenAI
OPENAI_API_KEY=sk-your-key-here

# Neo4j Graph Storage
NEO4J_URI=bolt://neo4j:7687
NEO4J_USER=neo4j
NEO4J_PASSWORD=password
NEO4J_HTTP_PORT=7474
NEO4J_BOLT_PORT=7687

# Application Services
EXTRACTION_SERVICE_PORT=8000
EXTRACTION_SERVICE_URL=http://extraction-service:8001
SEMANTIC_SERVICE_PORT=8001
CHAT_INTERFACE_PORT=8501
NEXT_APP_PORT=3000

# Opik Observability (used with --profile opik)
OPIK_VERSION=latest
OPIK_URL=http://opik-backend:8080/api
OPIK_PROJECT_NAME=seocho
OPIK_MYSQL_PASSWORD=opik
OPIK_CH_USER=opik
OPIK_CH_PASSWORD=opik
OPIK_MINIO_USER=opik
OPIK_MINIO_PASSWORD=opikopik

# Miscellaneous
TIMEZONE=UTC
